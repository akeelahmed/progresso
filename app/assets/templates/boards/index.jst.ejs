<% this.collection.each(function(model) { %>
   <li class="boards__board-link">
       <a href="#boards/<%= model.id %>">
          <%= model.get('name') %>
       </a>
   </li>
<% }) %>
<li class="boards__board--new">
    <a class="boards__board--new__open-button">
        Create New Board!
    </a>
</li>
